<%  add_breadcrumb_item feature_link(object.feature)
    add_breadcrumb_item link_to(Affiliation.model_name.human(count: :many).s, collection_path)
    add_breadcrumb_item ts('new.this') %>
<section class="panel panel-content">
  <div class="panel-heading">
  <h6><%= ts :for, what: t('creat.ing', what: t('new.record', what: Affiliation.model_name.human.titleize)), whom: "#{Feature.model_name.human.titleize} #{parent_object}" %></h6>
  </div>
  <div class="panel-body">
<br class="clear"/>
<%= form_for [:admin, object.feature, object] do |f| %>
<%=   render partial: 'form_fields', locals: {f: f, object: object} %>
<%=   link_to ts('cancel.this'), admin_feature_path(parent_object.fid), class: 'btn btn-primary form-submit', id: 'edit-cancel' %> | <%= globalized_submit_tag 'creat.e.this', class: 'btn btn-primary form-submit' %>
<%  end %>
  </div> <!-- END panel-body -->
</section> <!-- END panel -->
