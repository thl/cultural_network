<% add_breadcrumb_item feature_link(object.feature)
   add_breadcrumb_item feature_names_link(object.feature)
   add_breadcrumb_item link_to(object.id, admin_feature_name_path(object))
   add_breadcrumb_item ts('edit.this') %>
<div><h1><%= ts 'edit.ing.record', :what => FeatureName.model_name.human.titleize.s %>: <%= object %></h1></div>
<br class="clear"/>
<section class="panel panel-content">
  <div class="panel-heading">
    <h6><%= FeatureName.model_name.human.titleize.s %></h6>
  </div>
  <div class="panel-body">
<%= form_with(model: [:admin, object],local: true) do |f| %>
<%=  render :partial=>'form_fields', :locals=>{:f=>f} %>
<%=  link_to ts('cancel.this'), admin_feature_name_path(object), class: 'btn btn-primary form-submit', id: 'edit-cancel' %> | <%= globalized_submit_tag 'update.this', class: 'btn btn-primary form-submit' %>
<% end %>
  </div> <!-- END panel-body -->
</section> <!-- END panel -->
