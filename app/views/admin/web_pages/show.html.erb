<% add_breadcrumb_item link_to(Citation.model_name.human.s, polymorphic_path([:admin, parent_object.citable, parent_object]))
   add_breadcrumb_item WebPage.model_name.human(:count => :many).s
   add_breadcrumb_item object.id %>
<fieldset>
  <legend><%= ts(:for, :what => WebPage.model_name.human(:count => :many).titleize.s, :whom => ts('citation.of', :what => object.citation.citable_type.titleize)) %> <%= edit_item_link object %> <%= delete_item_link object %></legend>
  <div class="row">
    <label><%= WebPage.human_attribute_name(:title).s %></label>
    <span><%= def_if_blank object, :title %></span>
  </div>
  <div class="row">
    <label><%= WebPage.human_attribute_name(:path).s %></label>
    <span><%= def_if_blank object, :full_path %></span>
  </div>
</fieldset>
