<% # if we're showing the names of a particular Feature
   if parent_object
     add_breadcrumb_item feature_link(parent_object)
   end
   add_breadcrumb_item FeatureGeoCode.model_name.human(:count => :many).s %>
<div>
  <div class="left"><%= resource_search_form %></div>
  <div class="right highlight">
<% if parent? %>
<%=  ts :for, :what => new_item_link([parent_object, :feature_geo_code]), :whom => feature_link(parent_object) %>
<% else %>
<%=  ts 'snippet.feature.geo_code_needs_resource' %>
<% end %>
  </div>
</div>
<br class="clear"/>
<section class="panel panel-content">
  <div class="panel-heading">
    <h6><%= FeatureGeoCode.model_name.human.pluralize.s %></h6>
  </div>
  <div class="panel-body">
<%= render :partial => 'list', :locals => { :list => @collection } %>
  </div> <!-- END panel-body -->
</section> <!-- END panel -->
